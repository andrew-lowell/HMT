  <tool name="change_asset" label="Change Asset" icon="PLASMA_App">
    <script scriptType="python"><![CDATA[def change_node_prefix(node,prefix,defs):
    

    prev_op    = node.type().name() 
    new_prefix = prefix + prev_op

    for definition in defs:
        if new_prefix == definition.nodeTypeName():
        
            print node.path()
            print 'previous type:', prev_op
            print 'switching to:', new_prefix,'\n'
            return node.changeNodeType(definition.nodeTypeName(), keep_name=True, keep_parms=True, keep_network_contents=False, force_change_on_node_type_match=False)
            break


print 'launching asset change prefix\n\n'
defs = hou.hda.definitionsInFile('G:/My Drive/Training/Music Tools/HMT/otls/music_toolset.v02.hda')

nodes = hou.node('/obj/').allSubChildren()
for node in nodes:
    try:
        change_node_prefix(node,'hmt_',defs)
        change_node_prefix(node,'hmt_internal_',defs)
    except:
        ""]]></script>
  </tool>

  <tool name="change_type" label="Change Type" icon="PLASMA_App">
    <script scriptType="python"><![CDATA[def change_type_to(node,prev,new):
    
    if node.type().name() == prev:
        
        print node.path()
        print 'previous type:', prev
        print 'switching to:', new,'\n'
        return node.changeNodeType(new, keep_name=True, keep_parms=True, keep_network_contents=False, force_change_on_node_type_match=False)

print 'launching asset change type\n\n'

nodes = hou.node('/obj/').allSubChildren()
for node in nodes:
    try:
        change_type_to(node,'music_viz','hmt_viz')
    except:
        ""]]></script>
  </tool>

